{
  "name": "mango-deliveries",
  "version": "2.0.0",
  "description": "Vagrant Logistics - EVE Online freighting service",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/bin/www.js",
  "scripts": {
    "postinstall": "cd client && npm install",
    "clean": "rimraf dist/*",
    "lint": "eslint \"**/*.ts\"",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "start": "node --harmony --trace-warnings .",
    "start:dev": "nodemon --harmony --trace-warnings --watch \"web/**\" --ext \"ts,json\" .",
    "dev:server": "npm-run-all build start:dev",
    "dev:client": "cd client && npm run dev",
    "dev": "npm-run-all --parallel dev:server dev:client"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DrSmugleaf/Mango-Deliveries.git"
  },
  "author": "Vagrant Logistics",
  "license": "MIT",
  "dependencies": {
    "async": "^3.2.5",
    "axios": "^1.6.8",
    "common-tags": "^1.8.2",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "moment-timezone": "^0.5.45",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "rimraf": "^5.0.5",
    "sequelize": "^6.37.1",
    "valid-url": "^1.0.9",
    "winston": "^3.12.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/valid-url": "1.0.7",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
