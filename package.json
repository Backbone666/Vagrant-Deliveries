{
  "name": "mango-deliveries",
  "version": "1.0.0",
  "description": "EVE Online freighting service",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/bin/www.js",
  "scripts": {
    "postinstall": "cd client && npm install",
    "clean": "rimraf dist",
    "lint": "eslint \"**/*.ts\"",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "start": "node --harmony --trace-warnings .",
    "start:dev": "nodemon --harmony --trace-warnings --watch \"src/**\" --ext \"ts,json\" .",
    "dev:server": "npm-run-all build start:dev",
    "dev:client": "cd client && npm run build:start",
    "dev": "npm-run-all --parallel dev:server dev:client"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DrSmugleaf/Mango-Deliveries.git"
  },
  "author": "DrSmugleaf",
  "license": "MIT",
  "dependencies": {
    "async": "^3.2.6",
    "axios": "^1.7.9",
    "common-tags": "^1.8.2",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "moment-timezone": "^0.5.46",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.5",
    "rimraf": "^6.0.1",
    "sequelize": "^6.37.5",
    "valid-url": "^1.0.9",
    "winston": "^3.17.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/cookie-parser": "^1.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.17.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@types/pug": "^2.0.10",
    "@types/sequelize": "^4.28.20",
    "@types/valid-url": "^1.0.7",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "benchmark": "^2.1.4",
    "chai": "^4.3.10",
    "eslint": "^8.57.0",
    "foreman": "^3.0.1",
    "mocha": "^11.0.1",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  }
}