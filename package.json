{
  "name": "mango-deliveries",
  "version": "2.0.0",
  "description": "Vagrant Logistics - EVE Online freighting service",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/bin/www.js",
  "scripts": {
    "postinstall": "cd client && npm install",
    "clean": "rimraf -g dist/*",
    "lint": "eslint \"**/*.ts\"",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "test": "jest",
    "start": "node --harmony --trace-warnings .",
    "start:dev": "nodemon --harmony --trace-warnings --watch \"web/**\" --ext \"ts,json\" .",
    "dev:server": "npm-run-all build start:dev",
    "dev:client": "cd client && npm run dev",
    "dev": "npm-run-all --parallel dev:server dev:client"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DrSmugleaf/Mango-Deliveries.git"
  },
  "author": "Vagrant Logistics",
  "license": "MIT",
  "dependencies": {
    "async": "3.2.6",
    "axios": "1.13.2",
    "common-tags": "1.8.2",
    "connect-session-sequelize": "7.1.7",
    "cookie-parser": "1.4.7",
    "dotenv": "16.6.1",
    "express": "4.22.1",
    "express-rate-limit": "8.2.1",
    "express-session": "1.18.2",
    "helmet": "7.2.0",
    "moment-timezone": "0.5.48",
    "morgan": "1.10.1",
    "mysql2": "3.16.0",
    "rimraf": "5.0.10",
    "sequelize": "6.37.7",
    "sqlite3": "5.1.7",
    "valid-url": "1.0.9",
    "winston": "3.19.0",
    "yaml": "2.8.2"
  },
  "devDependencies": {
    "@types/async": "3.2.25",
    "@types/cookie-parser": "1.4.10",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.25",
    "@types/express-rate-limit": "5.1.3",
    "@types/express-session": "1.18.2",
    "@types/jest": "30.0.0",
    "@types/morgan": "1.9.10",
    "@types/node": "20.19.29",
    "@types/valid-url": "1.0.7",
    "@types/validator": "13.15.10",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "30.2.0",
    "nodemon": "3.1.11",
    "npm-run-all": "4.1.5",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.20.0"
  }
}
